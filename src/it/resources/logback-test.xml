<configuration>

    <appender name="CONSOLE" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>[%level] %msg%n</pattern>
        </encoder>
    </appender>

    <logger name="com.adform.sprint" level="info" />

    <root level="warn">
        <appender-ref ref="CONSOLE" />
    </root>

</configuration>
